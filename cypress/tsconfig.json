// ./cypress/tsconfig.json (extends main root config).
{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "@src/*": ["../src/*"],
      "pages/*": ["pages/*"]
    },
    "types": [
      "cypress"
    ]
  },
  "include": [
    "**/*.*"
  ]
}
